{
  "name": "manantena-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "heroku-postbuild": "npm run build-locale",
    "extract-i18n": "ng xi18n manantena-frontend --i18n-format xlf --output-path i18n --i18n-locale en && ng run manantena-frontend:xliffmerge",
    "start-pl": "ng serve --configuration=pl",
    "build-locale:en": "ng build --prod --i18n-locale en --i18n-format xlf --i18n-file src/i18n/messages.en.xlf --output-path=dist/en --baseHref /en/",
    "build-locale:pl": "ng build --prod --i18n-locale pl --i18n-format xlf --i18n-file src/i18n/messages.pl.xlf --output-path=dist/pl --baseHref /pl/",
    "build-locale": "npm run build-locale:en && npm run build-locale:pl"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.4",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.9",
    "@angular/forms": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@ngx-i18nsupport/tooling": "^1.1.2",
    "core-js": "^2.6.9",
    "express": "^4.17.1",
    "path": "^0.12.7",
    "primeicons": "^1.0.0",
    "primeng": "^7.1.3",
    "rxjs": "~6.3.3",
    "tslib": "^1.10.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "enhanced-resolve": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "webpack-dev-server": "^3.8.1"
  },
  "engines": {
    "node": "10.12.0",
    "npm": "6.8.0"
  }
}
